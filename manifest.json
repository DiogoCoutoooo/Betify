{
	"description": "QoL improvments to betting websites",
	"manifest_version": 3,
	"name": "Betify",
	"version": "5.2",
	"homepage_url": "https://github.com/DiogoCoutoooo/Betify",
	"icons": {
		"96": "betify-logo-96.svg"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.betano.pt/*",
				"*://*.betclic.pt/*",
				"*://*.bwin.pt/*",
				"*://*.estorilsolcasinos.pt/*"
			],
			"css": [
				"style.css"
			],
			"js": [
				"betify.js"
			]
		}
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "betify@test.com"
		}
	}
}